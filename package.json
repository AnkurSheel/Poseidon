{
    "version": "1.3.5",
    "name": "newt",
    "main": "./dist/main.bundle.js",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "license": "MIT",
    "description": "A app to view networth changes",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/AnkurSheel/Poseidon.git"
    },
    "keywords": [
        "electron",
        "typescript",
        "react",
        "webpack",
        "sqlite3"
    ],
    "scripts": {
        "start": "electron .",
        "dev": "rimraf dist && webpack --config ./config --progress --colours",
        "staging": "rimraf dist webpack --config ./config --progress --colours",
        "watch": "webpack-dev-server --port 3000 --config ./config --progress --colours --remote-debugging-port=9223",
        "watch:staging": "webpack-dev-server --port 3000 --config ./config --progress --colours --remote-debugging-port=9223",
        "prod": "rimraf dist && webpack --config ./config",
        "postinstall": "electron-builder install-app-deps",
        "build": "npm run prod &&  electron-builder . --config ./electron-builder.json --publish never",
        "semantic-release": "semantic-release",
        "commit": "yarn git-cz"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "build": {
        "npmRebuild": "true"
    },
    "dependencies": {
        "@date-io/moment": "^1.3.5",
        "@material-ui/core": "^3.9.3",
        "electron-devtools-installer": "^2.2.4",
        "electron-log": "^3.0.5",
        "electron-updater": "^4.0.6",
        "knex": "^0.16.5",
        "material-ui-pickers": "^2.2.4",
        "moment": "^2.24.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.0.0",
        "recharts": "^1.5.0",
        "recompose": "^0.30.0",
        "sqlite3": "^4.0.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "@semantic-release/changelog": "^3.0.2",
        "@semantic-release/exec": "^3.3.2",
        "@semantic-release/git": "^7.0.8",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/react": "^16.8.14",
        "@types/react-dom": "^16.8.4",
        "@types/react-router-dom": "^4.3.2",
        "@types/recharts": "^1.1.16",
        "@types/recompose": "^0.30.6",
        "@types/sqlite3": "^3.1.5",
        "commitizen": "^3.1.1",
        "css-loader": "^2.1.1",
        "cz-conventional-changelog": "^2.1.0",
        "electron": "^5.0.0",
        "electron-builder": "^20.39.0",
        "emoji-cz": "^0.3.0",
        "env-cmd": "^8.0.2",
        "faker": "^4.1.0",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^2.1.0",
        "mini-css-extract-plugin": "^0.6.0",
        "node-sass": "^4.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "react-hot-loader": "^4.8.4",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "semantic-release": "^15.13.3",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.1",
        "ts-loader": "^5.4.3",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.1",
        "webpack-dev-server": "^3.3.1",
        "webpack-merge": "^4.2.1",
        "webpack-notifier": "^1.7.0"
    }
}
